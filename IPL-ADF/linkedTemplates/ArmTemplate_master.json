{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "IPL-ADF"
		},
		"IPL_adls_project_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'IPL_adls_project'"
		},
		"github_adf_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'github_adf_ls'"
		},
		"IPL_adls_project_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsiplproject22.dfs.core.windows.net/"
		},
		"github_adf_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/Kinfash2000/King_adf_adb_project/raw/refs/heads/main/"
		},
		"github_adf_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kinfash2000"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "IPL-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"IPL_adls_project_accountKey": {
						"value": "[parameters('IPL_adls_project_accountKey')]"
					},
					"github_adf_ls_password": {
						"value": "[parameters('github_adf_ls_password')]"
					},
					"IPL_adls_project_properties_typeProperties_url": {
						"value": "[parameters('IPL_adls_project_properties_typeProperties_url')]"
					},
					"github_adf_ls_properties_typeProperties_url": {
						"value": "[parameters('github_adf_ls_properties_typeProperties_url')]"
					},
					"github_adf_ls_properties_typeProperties_userName": {
						"value": "[parameters('github_adf_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}